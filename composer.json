{
    "name": "quinninteractive/silverstripe-seo",
    "description": "An all-in-one SEO module for SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "seo",
        "facebook",
        "twitter",
        "opengraph",
        "search",
        "optimization",
        "optimisation"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Reece Alexander",
            "homepage": "https://vulcandigital.co.nz",
            "role": "author"
        },
        {
            "name": "Fred Condo",
            "homepage": "https://quinn.com",
            "role": "maintainer"
        },
        {
            "name": "Phil Quinn",
            "homepage": "https://quinn.com",
            "role": "maintainer"
        }
    ],
    "require": {
        "php": "^8.3",
        "jonom/silverstripe-text-target-length": "^2.0",
        "kub-at/php-simple-html-dom-parser": "^1.9",
        "silverstripe/cms": "^6",
        "silverstripe/asset-admin": "*",
        "wilr/silverstripe-googlesitemaps": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "rector/rector": "^1.2",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "lint": [
            "vendor/bin/phpcs"
        ],
        "rector": [
            "rector process --config rector.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "QuinnInteractive\\Seo\\": "src/",
            "QuinnInteractive\\Seo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "3.x-dev"
        },
        "expose": [
            "dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
